<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AnoLabelView">
        <attr name="label_horizontalSpace" format="dimension" />
        <attr name="label_verticalSpace" format="dimension" />
        <attr name="label_maxLines" format="integer" />
        <attr name="label_item_background" format="reference|color" />
        <attr name="label_item_textColor" format="reference|color" />
        <attr name="label_item_textSize" format="dimension" />
        <attr name="label_item_padding" format="dimension" />
        <attr name="label_item_paddingLeft" format="dimension" />
        <attr name="label_item_paddingTop" format="dimension" />
        <attr name="label_item_paddingRight" format="dimension" />
        <attr name="label_item_paddingBottom" format="dimension" />
        <attr name="label_check_type" format="enum">
            <enum name="none" value="1" />
            <enum name="single" value="2" />
            <enum name="multi" value="3" />
        </attr>
        <attr name="label_check_maxNum" format="integer" />
    </declare-styleable>
</resources>